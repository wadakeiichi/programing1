{
  "jupyter.kernelPickerType": "mru", 
  "python.defaultInterpreterPath": "/usr/local/bin/python",
  "jupyter.jupyterServerType": "local",

  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "j"],
      "after": ["<Esc>"]
    }
  ],
  "vim.hlsearch": true,
  "vim.startInInsertMode": true,

  "[notebook]": {
    "vim.enable": false
  }
}